{
  "app": "npx ts-node --prefer-ts-exts bin/backend.ts",
  "watch": {
    "include": [
      "**"
    ],
    "exclude": [
      "README.md",
      "cdk*.json",
      "**/*.d.ts",
      "**/*.js",
      "tsconfig.json",
      "package*.json",
      "yarn.lock",
      "node_modules",
      "test"
    ]
  },
  "requireApproval": "never",
  "context": {
    "config": {
      "tableName": "theater-schedule-book",
      "domain": "theater-schedule-book",
      "callbackUrls": [
        "http://localhost:3000"
      ],
      "logoutUrls": [
        "http://localhost:3000"
      ]
    }
  }
}
