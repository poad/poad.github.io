{
  "name": "backend",
  "version": "0.1.0",
  "bin": {
    "backend": "bin/backend.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "cdk": "cdk",
    "format": "prettier --write **/*.cts **/*.ts",
    "format-check": "prettier --check **/*.cts **/*.ts",
    "lint": "eslint --ext .ts ./bin ./lib",
    "lint-fix": "eslint --fix --ext .ts ./bin ./lib",
    "prelint": "prettier --check **/*.ts",
    "prelint-fix": "prettier --write **/*.ts"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@types/node": "^20.4.2",
    "aws-cdk": "^2.87.0",
    "eslint": "^8.45.0",
    "eslint-config-import": "^0.13.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^5.0.0",
    "prettier": "^3.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "aws-cdk-lib": "^2.88.0",
    "constructs": "^10.2.69",
    "source-map-support": "^0.5.21"
  }
}
